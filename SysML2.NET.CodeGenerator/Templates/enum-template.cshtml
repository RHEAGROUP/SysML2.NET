namespace SysML2.NET
{
	<gen>/// <summary>
	@foreach (var docline in @Model.QueryDocumentation())
	{
	<code>/// @docline</code> 
	}
	/// </summary></gen>
	public enum @Model.Name
	{
		@foreach (var enumLiteration in @Model.ELiterals)
		{
			<gen>/// <summary>
			@foreach (var docline in @enumLiteration.QueryDocumentation())
			{
			<code>/// @docline</code> 
			}
			/// </summary></gen>
			<gen>@enumLiteration.Name.CapitalizeFirstLetter() = @enumLiteration.Value,
			</gen>
		}
	}
}
